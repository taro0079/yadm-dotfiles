{
  "filename": {
    "prefix": ["fname"],
    "body": "$TM_FILENAME_BASE"
  },
  "user": {
    "prefix": "username",
    "body": "${VIM:\\$USER}"
  },
  "uuid": {
    "prefix": "uuid",
    "body": [
      "${VIM:system('python3 -c \"import uuid, sys;sys.stdout.write(str(uuid.uuid4()))\"')}"
    ]
  }
}
